# Odoo Configuration
# Copy this file to .env and fill in your values

# Odoo Server
ODOO_HOST=localhost
ODOO_PORT=8069

# Database
DATABASE=odoo

# Odoo Admin Password (for installing/upgrading modules)
ADMIN_PASSWORD=admin

# Test Configuration
ODOO_USER=test_user
ODOO_USER_PASSWORD=test_password

# Module Name (usually matches the folder name)
MODULE_NAME=ambassador_coupons

# Test Tags
TEST_TAGS=ambassador_coupons

# Paths (adjust if needed)
ODOO_BIN=odoo-bin
